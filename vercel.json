{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3"
    },
    "api/test.ts": {
      "runtime": "@vercel/node@3"
    },
    "api/minimal.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/minimal",
      "dest": "/api/minimal.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}