{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/location/home/([^/]+)",
      "dest": "/api/location/home/[userId]?userId=$1"
    },
    {
      "src": "/api/location/validate",
      "dest": "/api/location/validate"
    },
    {
      "src": "/api/location/parse",
      "dest": "/api/location/parse"
    },
    {
      "src": "/api/location/home",
      "dest": "/api/location/home"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}