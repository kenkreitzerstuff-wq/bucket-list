{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/location/validate",
      "destination": "/api/validate"
    },
    {
      "source": "/api/location/parse",
      "destination": "/api/parse"
    },
    {
      "source": "/api/location/home",
      "destination": "/api/home"
    },
    {
      "source": "/api/location/home/(.*)",
      "destination": "/api/home/$1"
    },
    {
      "source": "/api/test",
      "destination": "/api/test-health"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}